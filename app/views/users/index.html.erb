<% if flash[:notice] %>
 <p><%= flash[:notice] %></p>
 <% end %>
<div class="users-index">
<header class="head-books-index">
 <div class="head-books-index2">
  <%= link_to "Bookers", root_path %>
 </div>
</header>
<div class="users-index-item">
<div class="sidebare">
<%= render 'books/info', user: current_user %>
<%= render 'books/new', book: @book %>
</div>
 <div class="main-users-index">
  <h2>Users</h2>
<table class="table">
<% @users.each do |user| %>
<thead>
 <tr>
 <th><%= attachment_image_tag current_user, :profile_image, :fill, 60, 60, fallback: "no_image.jpg" %></th>
 <th><%= user.name %></th>
 <th><%= link_to "show", user_path(user) %></th>
 </tr>
</thead>
<% end %>
</div>
</div>
</div